{
  "name": "clawbrowser",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:frontend": "vite",
    "build:frontend": "vite build",
    "build:sidecar": "cd sidecar && npx tsc",
    "dev:sidecar": "cd sidecar && npx tsc --watch",
    "dev:tauri": "VITE_SIDECAR_NAME=sidecar/clawbrowser-agent-dev tauri dev",
    "tauri": "tauri",
    "dev": "CLAW_LOG_DIR=./log npm run build:sidecar && CLAW_LOG_DIR=./log concurrently -k -n sidecar,tauri -c blue,green \"npm run dev:sidecar\" \"npm run dev:tauri\"",
    "build": "npm run build:sidecar && tauri build",
    "test": "vitest run --coverage --config vitest.config.ts && node sidecar/tools/analyze-browser-automation.js"
  },
  "devDependencies": {
    "@tauri-apps/api": "^2",
    "@tauri-apps/cli": "^2",
    "@vitest/coverage-v8": "^4.0.18",
    "concurrently": "^9.2.1",
    "jsdom": "^28.1.0",
    "typescript": "^5.7.0",
    "vite": "^6.0.0",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "@tauri-apps/plugin-shell": "^2.3.5"
  }
}
