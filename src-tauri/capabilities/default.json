{
  "identifier": "default",
  "description": "Default permissions for ClawBrowser",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:event:default",
    {
      "identifier": "shell:allow-spawn",
      "allow": [
        {
          "name": "sidecar/clawbrowser-agent",
          "sidecar": true
        },
        {
          "name": "sidecar/clawbrowser-agent-dev",
          "sidecar": true
        }
      ]
    },
    "shell:allow-stdin-write",
    "shell:allow-kill",
    "shell:allow-open"
  ]
}
